{
  "name": "lintier",
  "version": "1.0.2",
  "description": "Setup Eslint & Prettier in Typescript projects.",
  "main": "./bin/index.js",
  "scripts": {
    "lint-setup": "node index.js",
    "build": "rimraf ./bin && tsc",
    "start": "npm run build && node bin/index.js",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx --ignore-path .gitignore .",
    "lint:fix": "eslint --ext .js,.jsx,.ts,.tsx --ignore-path .gitignore --fix ."
  },
  "bin": {
    "lintier": "./bin/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Josh-Stillman/ts-eslint-prettier-install.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Josh-Stillman/ts-eslint-prettier-install/issues"
  },
  "homepage": "https://github.com/Josh-Stillman/ts-eslint-prettier-install#readme",
  "dependencies": {
    "commander": "6.2.0",
    "execa": "4.1.0",
    "inquirer": "7.3.3"
  },
  "devDependencies": {
    "@types/commander": "2.12.2",
    "@types/inquirer": "7.3.1",
    "@types/node": "14.14.10",
    "@typescript-eslint/eslint-plugin": "4.9.0",
    "@typescript-eslint/parser": "4.9.0",
    "eslint": "7.2.0",
    "eslint-config-airbnb": "18.2.1",
    "eslint-config-prettier": "6.15.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "3.1.4",
    "eslint-plugin-react": "7.21.5",
    "eslint-plugin-react-hooks": "4.0.0",
    "prettier": "2.2.1",
    "rimraf": "3.0.2",
    "typescript": "4.1.2"
  }
}
